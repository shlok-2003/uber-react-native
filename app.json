{
    "expo": {
        "name": "Uber",
        "slug": "uber",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "uber",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#2F80ED"
        },
        "platforms": ["ios", "android"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "permissions": [
                "android.permission.ACCESS_COARSE_LOCATION",
                "android.permission.ACCESS_FINE_LOCATION"
            ],
            "package": "com.shlok_03.uber"
        },
        "web": {
            "bundler": "metro",
            "output": "server",
            "favicon": "./assets/images/icon.png"
        },
        "plugins": [
            [
                "expo-router",
                {
                    "origin": "https://uber.com"
                }
            ],
            "expo-secure-store",
            [
                "expo-location",
                {
                    "locationAlwaysAndWhenInUsePermission": "Allow Uber to use your location."
                }
            ],
            [
                "@stripe/stripe-react-native",
                {
                    "merchantIdentifier": "merchant.com.uber",
                    "enableGooglePay": true,
                    "publishableKey": "pk_test_51QlTnkQSfGut1ZmOR6UVin71f9UlhRVJ6OoL3eOY7mXgvOJHF6NU3FX3Jmqv9XcqeDWJCUDwJ3lwBATyIPeO1AgM00IMJi5mjN"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": "https://uber.com"
            },
            "eas": {
                "projectId": "0f8d73f3-39ca-4deb-b4f2-7ca7f72fdf67"
            }
        }
    }
}
